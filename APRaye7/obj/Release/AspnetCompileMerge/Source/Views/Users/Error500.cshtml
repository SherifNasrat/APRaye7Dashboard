@using APRaye7.App_GlobalResources
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = Users_resource.PageTitle;
    ViewBag.SubTitle = Users_resource.Error500;
}

@section Menu {
    @Scripts.Render("~/Scripts/Examples/P500/menu")
}

<div class="error-page">
    <h2 class="headline text-red">500</h2>
    <div class="error-content">
        <h3><i class="fa fa-warning text-red"></i> Oops! Something went wrong.</h3>
        <p>
            We will work on fixing that right away.
            Meanwhile, you may <a href="@Url.Action("Index","Home")">return to dashboard.</a> 
        </p>
    </div>
</div>



